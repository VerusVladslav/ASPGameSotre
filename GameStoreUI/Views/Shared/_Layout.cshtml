<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />
    <link href="~/Content/adminite.css" rel="stylesheet" />
    <link href="~/Content/Layoutstyle.css" rel="stylesheet" />

</head>
<body>
    <div class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                @{ Html.RenderPartial("Header"); }
               
            </div>
           
        </div>
    </div>
    <div class="container body-content">
        @RenderBody()

    </div>

    <script src="https://kit.fontawesome.com/41174f93d5.js" crossorigin="anonymous"></script>

    <script src="~/Scripts/jquery-3.4.1.js"></script>

    <script src="~/Scripts/popper.js"></script>


    <script src="~/Scripts/bootstrap.min.js"></script>
    <script src="~/Scripts/adminite.js"></script>
    <script src="~/Scripts/script.js"></script>
</body>
</html>
